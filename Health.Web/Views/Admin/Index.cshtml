@model IEnumerable<Heartbeat>
@{
    ViewBag.Title = "Admin Area";
    var data = (User)ViewData["UserInfo"];
}

<div class="container">
    <div class="row">
        <div>Benvenuto @data.Name @data.Surname nella tua Area Admin</div>
        <div class="col-md-6">
            <button type="button" onclick="location.href='@Url.Action("ShowUser", "Signup" )'" />Modifica i tuoi dati</button>
        </div>

    </div>
</div>


<div class="container">
    <div class="row">
        <div class="col-md-6">
            <button type="button" onclick="location.href='@Url.Action("ShowUsers", "Admin" )'" />Modifica ruoli iscritti</button>
        </div>
        <div class="col-md-6">

            <button type="submit" formaction="URL">Associa dottori</button>

        </div>
    </div>
</div>




